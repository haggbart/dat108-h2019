<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>F09f - web.xml og initparams</display-name>
  <welcome-file-list>
    <welcome-file>greetings</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Min magiske Servlet</servlet-name>
    <servlet-class>somepackage.MyServlet</servlet-class>
    <init-param>
      <param-name>greeting</param-name>
      <param-value>Heia Varegg!</param-value>
    </init-param>
  </servlet>
  <context-param>
    <param-name>greeting</param-name>
    <param-value>Hello world!</param-value>
  </context-param>
</web-app>